<%
	int num = 90;
	char grade = '';
	if(num > 89){
		grade = 'A';
	}else if(num > 79){
		grade = 'B';
	}else if(num > 69){
		grade = 'C';
	}else if(num > 59){
		grade = 'D';
	}else {
		grade = 'F';
	}
%>
<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Insert title here</title>
</head>
<body>
	<%
	int num = 90;
	if (num > 89) {
	%>
	<p>A학점</p>
	<%
	} else if (num > 79) {
	%>
	<p>B학점</p>
	<%
	} else if (num > 69) {
	%>
	<p>C학점</p>
	<%
	}
	%>

	<p></p>
</body>
